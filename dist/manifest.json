{
  "name": "m42ar",
  "version": "0.0.2",
  "description": "Removes ads from Mind42",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["app.js"],
    "persistent": true
  }
}
